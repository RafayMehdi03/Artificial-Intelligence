      "source": [
        "def calculate_average(marks_list):\n",
        "    total = sum(marks_list)\n",
        "    count = len(marks_list)\n",
        "    average = total / count\n",
        "    return average\n",
        "\n",
        "my_marks = [90, 70, 80]\n",
        "result = calculate_average(my_marks)\n",
        "print(\"The average is:\", result)"
      ]
